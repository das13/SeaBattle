<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.text.*?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>

<?import javafx.scene.layout.*?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="360.0" minWidth="640.0" prefHeight="360.0" prefWidth="640.0"
      xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.CommonWindowController">
   <children>
      <HBox id="InfoPanel" maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="640.0">
         <children>
            <Label text="Login:">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="lblLogin" minWidth="20.0" text="MyLogin" wrapText="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label text="Wins:">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="lblWins" text="MyWins">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label text="Loses:">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="lblLoses" text="MyLoses">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Bold" size="13.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox minHeight="250.0" minWidth="640.0">
         <children>
            <AnchorPane id="ChatPanel" nodeOrientation="LEFT_TO_RIGHT" prefHeight="250.0" prefWidth="320.0">
               <children>
                  <TextArea fx:id="txaChat" editable="false" layoutY="20.0" prefHeight="230.0" prefWidth="320.0" promptText="Chat..." wrapText="true" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                  <Label text="Chat:" AnchorPane.leftAnchor="0.0">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
               </HBox.margin>
            </AnchorPane>
            <AnchorPane id="ActivePanel" maxWidth="200.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label alignment="TOP_CENTER" text="Free gamers:" AnchorPane.leftAnchor="0.0">
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font>
                  </Label>
                  <TableView fx:id="tblActiveGamers" layoutY="20.0" prefHeight="195.0" prefWidth="180.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <columns>
                      <TableColumn fx:id="colActiveNicks" maxWidth="-1.0" minWidth="95.0" prefWidth="95.0" text="Nick" />
                      <TableColumn fx:id="colActiveWins" maxWidth="-1.0" minWidth="45.0" prefWidth="45.0" text="Wins" />
                        <TableColumn fx:id="colActiveLoses" maxWidth="-1.0" minWidth="45.0" prefWidth="45.0" text="Loses" />
                    </columns>
                  </TableView>
                  <Button fx:id="btnAtack" contentDisplay="CENTER" layoutY="215.0" mnemonicParsing="false" onAction="#pressBtnAtack" prefHeight="35.0" prefWidth="180.0" text="Attack!" textFill="#d01c1c" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </AnchorPane>
            <AnchorPane id="PassivePanel" maxWidth="80.0" prefHeight="200.0" prefWidth="80.0">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
               <children>
                  <Label text="In game:" AnchorPane.leftAnchor="0.0">
                     <font>
                        <Font name="Arial" size="13.0" />
                     </font>
                  </Label>
                  <TableView fx:id="tblPassiveGamers" layoutY="20.0" prefHeight="230.0" prefWidth="80.0" AnchorPane.rightAnchor="0.0">
                    <columns>
                      <TableColumn fx:id="colPassiveNicks" maxWidth="-1.0" minWidth="65.0" prefWidth="65.0" text="Nick" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
         </children>
      </HBox>
      <AnchorPane prefHeight="60.0" prefWidth="360.0">
         <children>
            <TextArea fx:id="txtMassage" nodeOrientation="LEFT_TO_RIGHT" prefHeight="50.0" prefWidth="620.0" promptText="Print your message here..." wrapText="true" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="5.0" />
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </AnchorPane>
   </children>
</VBox>
